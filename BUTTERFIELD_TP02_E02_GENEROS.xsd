<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="generos">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="genero" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id" type="xs:integer"/>
              <xs:element name="nombre" type="xs:string"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
